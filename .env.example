# LifeOS Autonomous System Configuration
# Copy this file to .env and fill in your actual values

# Notion Integration (Required)
NOTION_API_TOKEN=your_notion_integration_token_here

# Notion Database IDs (Auto-detected if not provided)
NOTION_CHANNELS_DATABASE_ID=your_youtube_channels_database_id
NOTION_KNOWLEDGE_DATABASE_ID=your_knowledge_hub_database_id

# Today's CC Page ID (Optional - for Today's CC monitoring)
TODAYS_CC_PAGE_ID=your_todays_cc_page_id

# Google API (Optional - for enhanced YouTube processing)
GOOGLE_API_KEY=your_google_api_key_here

# Gemini AI (Optional - for AI content analysis)
GEMINI_API_KEY=your_gemini_api_key_here

# System Configuration (Optional)
CC_CHECK_INTERVAL=30
YOUTUBE_CHECK_INTERVAL=30
AUTOMATION_CYCLE_INTERVAL=600
EOF < /dev/null
